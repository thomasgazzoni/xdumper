# xdumper configuration
# Copy this file to .env and fill in your values

# Path to accounts database (default: ~/.xdumper/accounts.db)
XDUMPER_DB=

# Path to tweet storage database (default: ~/.xdumper/tweets.db)
XDUMPER_STORE=

# Log level: DEBUG, INFO, WARNING, ERROR (default: WARNING)
XDUMPER_LOG_LEVEL=WARNING

# Proxy URL for Twitter API requests (optional)
# Examples:
#   socks5://127.0.0.1:1080
#   http://127.0.0.1:8080
#   http://user:pass@proxy.example.com:8080
XDUMPER_PROXY=
